def Sqr(a):
    for i in range(0, len(a)):
        a[i] = a[i] ** 2
    print("Squares of all the elements in the List =", a)


def Sum(a):
    b = 0
    for i in range(0, len(a)):
        b = a[i] + b
    print("Sum of all the square elements in the List =", b)


def Mean(a):
    print("Mean of all the square elements in the List =", sum(a) / len(a))


a = []
for i in range(1, 21):
    a.append(i)

Sqr(a)
Sum(a)
Mean(a)
